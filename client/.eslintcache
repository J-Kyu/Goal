n\n\n    //generate list of algo lines\n    const renderLines = algoList.map((algo,index)=>{\n      \n        return<Row gutter={4} key={algo._id}>\n                <div style={{ margin:\"5px\" }}>\n                <Space>\n                    <Button type=\"primary\" onClick={(e)=>ActionButton(algo._id,e)}>Confirm</Button>\n                    <a href={algo.link}>{algo.title}</a>\n                    <span>{algo.level}</span>\n                </Space>\n                </div>\n            </Row>\n    })\n\n\n    //render page\n    return (\n        <div style={{width:\"100%\" }}>\n            {renderLines}\n        </div>\n    )\n}\n\nexport default AlgorithmContent",{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","severity":1,"message":"45","line":1,"column":8,"nodeType":"46","messageId":"47","endLine":1,"endColumn":12},{"ruleId":"44","severity":1,"message":"48","line":11,"column":3,"nodeType":"46","messageId":"47","endLine":11,"endColumn":7},{"ruleId":"44","severity":1,"message":"49","line":13,"column":12,"nodeType":"46","messageId":"47","endLine":13,"endColumn":16},{"ruleId":"44","severity":1,"message":"50","line":2,"column":25,"nodeType":"46","messageId":"47","endLine":2,"endColumn":28},"no-native-reassign",["51"],"no-negated-in-lhs",["52"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'Link' is defined but never used.","'year' is assigned a value but never used.","'Col' is defined but never used.","no-global-assign","no-unsafe-negation"]